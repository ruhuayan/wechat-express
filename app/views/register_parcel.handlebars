<div class="weui-form register_parcel-form">
    <div class="weui-form__text-area">
        <h2 class="weui-form__title">{{title}}</h2>
        <div class="weui-form__desc"></div>
    </div>
    <div class="weui-form__control-area">
        <div class="weui-cells__group weui-cells__group_form">
            <div class="weui-cells weui-cells_form">
                <div class="weui-cell">
                    <div class="weui-cell__hd"><label class="weui-label">单号:</label></div>
                    <div class="weui-cell__bd">
                        <input id="js_series" class="weui-input" placeholder="填写单号">
                    </div>
                    <div class="weui-cell__ft">
                        <button class="weui-btn" id="js-scan" onclick="scan()">
                            <div class="weui-grid__icon">
                                <img src="/img/scan.svg" alt="扫码">
                            </div>
                        </button>
                    </div>
                </div>
                <div class="weui-cell">
                    <div class="weui-cell__hd"><label class="weui-label">重量:</label></div>
                    <div class="weui-cell__bd">
                        <input id="js_weight" type="number" class="weui-input" placeholder="填写重量" pattern="[0-9]*">
                    </div>
                    <div class="weui-cell__ft">
                        <label class="weui-label">公斤</label>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="weui-form__opr-area">
        <a class="weui-btn weui-btn_primary weui-btn_disabled" href="javascript:" id="showTooltips">确定</a>
    </div>
</div>